{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "nodejs-testing.extensions": [
        {
            "extensions": [
                "ts"
            ],
            "parameters": [
                "--import",
                "tsx"
            ]
        }
    ],
    "nodejs-testing.include": [
        "packages/*/test/"
    ]
}